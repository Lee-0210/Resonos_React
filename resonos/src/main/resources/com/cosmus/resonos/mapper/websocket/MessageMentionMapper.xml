<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cosmus.resonos.mapper.websocket.MessageMentionMapper">
    <insert id="insert" parameterType="com.cosmus.resonos.domain.websocket.MessageMention">
        INSERT INTO message_mentions(message_id, mentioned_user_id, created_at)
        VALUES(#{messageId}, #{mentionedUserId}, NOW())
    </insert>
</mapper>
